import java.util.Scanner;

public class Tabuada {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int A = 1, numero = sc.nextInt(), R;

        for (int i=0; A < 10;i++){
            R = A * numero;
            System.out.println(A +" x "+ numero+ " = "+ R);
            A++;
        }
    }
}
