import java.util.Scanner;

public class  {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int N, Totn = 0;

        for (int i = 0; i < 5; i++) {
            System.out.println("Digite um número: ");
            N = sc.nextInt();
            if (N < 0) {
                Totn++;
            }
        }

        System.out.println("Foram digitados " + Totn + " números negativos.");
    }
}
