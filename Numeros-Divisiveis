import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int C = 1, N = 0, contDiv = 0;
        System.out.println("Digite um numero: ");
        N = sc.nextInt();

        for (int i = 0; i <= N; i++) {
            if (N % C == 0) {
                contDiv++;
            }
        }

        System.out.println("Ao todo existem " + contDiv + " Valores divisiveis.");
    }
}
